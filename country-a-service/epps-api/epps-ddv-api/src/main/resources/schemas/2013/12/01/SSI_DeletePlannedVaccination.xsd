	</element>
	<complexType name="DeletePlannedVaccinationType">
		<sequence>
			<element name="PlannedVaccinationIdentifier" type="vaccinationcard:PlannedVaccinationIdentifierType" />
		</sequence>
	</complexType>
</schema>
