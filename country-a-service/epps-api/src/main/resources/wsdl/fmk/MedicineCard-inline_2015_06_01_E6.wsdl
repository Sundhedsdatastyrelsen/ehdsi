			<xs:element maxOccurs="unbounded" name="DrugMedication" type="medicinecard20150601:PausedDrugMedicationType"/>
		</xs:sequence>
	</xs:complexType>

	<xs:element name="DosageStartedPreviously" type="medicinecard20150601:DosageStartedPreviouslyType">
		<xs:annotation>
			<xs:documentation xml:lang="en-GB">Indicates that the dosage start is previously but not given on FMK</xs:documentation>
			<xs:documentation xml:lang="da-DK">Angiver at doseringen er startet tidligere, men ikke angivet på FMK</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="DosageStartedPreviouslyType"/>

	
	
	
