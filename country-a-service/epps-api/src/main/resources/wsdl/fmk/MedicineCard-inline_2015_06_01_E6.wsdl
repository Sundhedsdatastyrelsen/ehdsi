		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="400"/>
		</xs:restriction>
	</xs:simpleType>

	
	
	<xs:element name="Created" type="medicinecard20150601:CreatedType">
