			<xs:documentation xml:lang="da-DK">Registrering af en relation mellem en patient og en organisation</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="RemovePatientOrganisationRelationType">
		<xs:sequence>
			<xs:element minOccurs="0" name="ModifiedBy" type="medicinecard20150601:ModificatorType"/>
