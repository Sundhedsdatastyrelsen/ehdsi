            <documentation xml:lang="en-GB">SDSD Whitelisting header.</documentation>
            <documentation xml:lang="da-DK">SDSD Whitelisting header.</documentation>
        </annotation>
    </element>
    <complexType name="WhiteListingHeader">
        <sequence>
