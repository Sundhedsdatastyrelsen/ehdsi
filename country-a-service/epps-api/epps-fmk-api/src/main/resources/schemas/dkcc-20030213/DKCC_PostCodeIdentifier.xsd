<?xml version="1.0" encoding="UTF-8"?>
<schema targetNamespace="http://rep.oio.dk/ebxml/xml/schemas/dkcc/2003/02/13/" xmlns:dkcc="http://rep.oio.dk/ebxml/xml/schemas/dkcc/2003/02/13/" xmlns="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="qualified" version="1.1">
	<element name="PostCodeIdentifier" type="dkcc:PostCodeIdentifierType">
		<annotation>
			<documentation>The postal departments nationwide postal code.</documentation>
		</annotation>
	</element>
	<simpleType name="PostCodeIdentifierType">
		<restriction base="string">
			<pattern value="[0-9]{4}"/>
		</restriction>
	</simpleType>
</schema>
