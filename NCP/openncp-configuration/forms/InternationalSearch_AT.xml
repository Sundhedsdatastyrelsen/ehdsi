   <ism:country code="AT"
                friendlyName="International Search Mask for Austria"
                label="label.ism.addressCountry">
      <ism:patientSearch friendlyName="Patient Identity Traits" scope="ALL">
         <ism:identifier type="NORMAL">
            <ism:id contextualDescription="healthCardNumber"
                    domain="2.16.17.710.780.1000.990.1"
                    format="^.{1,}$"
                    label="label.ism.healthCardNumber"/>
