				<xs:element minOccurs="0" name="ConsentGiven" type="medicinecard20150601:ConsentWasGivenType"/>
				<xs:element minOccurs="0" name="ConsentRemoved" type="medicinecard20150601:ConsentWasRemovedType"/>
