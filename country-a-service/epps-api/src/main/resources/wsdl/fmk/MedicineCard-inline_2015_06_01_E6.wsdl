			<xs:element minOccurs="0" name="VersionMismatchWarning" type="medicinecard20150601:VersionMismatchWarningType"/>
		</xs:sequence>
	</xs:complexType>

	<xs:element name="IndicationFreeText" type="medicinecard20150601:IndicationFreeTextType">
		<xs:annotation>
			<xs:documentation xml:lang="en-GB">Indication text in free form</xs:documentation>
			<xs:documentation xml:lang="da-DK">Indikationstekst som fritekst</xs:documentation>
		</xs:annotation>
	</xs:element>
