			<xs:element maxOccurs="unbounded" name="Prescription">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="PrescriptionIdentifier" type="medicinecard20150601:PrescriptionIdentifierType"/>
						<xs:element name="OrderIdentifier" type="medicinecard20150601:OrderIdentifierType"/>
						<xs:element name="EffectuationIdentifier" type="medicinecard20150601:EffectuationIdentifierType"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:element name="INREndDate" type="medicinecard20150601:INREndDateType">
		<xs:annotation>
			<xs:documentation xml:lang="en-GB">The End date of the period wherein the INR-interval is relevant</xs:documentation>
			<xs:documentation xml:lang="da-DK">Slutdato for den periode hvor INR-intervallet er relevant</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:simpleType name="INREndDateType">
		<xs:restriction base="xs:date">
            <xs:minInclusive value="2000-01-01Z"/>
		</xs:restriction>
	</xs:simpleType>

	
	<xs:element name="PrescriptionReplicationStatus" type="medicinecard20150601:PrescriptionReplicationStatusType">
		<xs:annotation>
			<xs:documentation xml:lang="en-GB">Structure containing status for the replication of prescriptions.</xs:documentation>
			<xs:documentation xml:lang="da-DK">En structur der indeholder status for replikeringen af recepter</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="PrescriptionReplicationStatusType">
		<xs:sequence>
			<xs:element name="LatestReplicationDateTime" type="medicinecard20150601:LatestReplicationDateTimeType"/>
		</xs:sequence>
	</xs:complexType>

	<xs:element name="UnwithdrawDrugMedicationOnUpdate" type="medicinecard20150601:UnwithdrawDrugMedicationOnUpdateType">
		<xs:annotation>
			<xs:documentation xml:lang="en-GB">Indicates that the drug medications is withdrawn and that it is request that its undone before updated </xs:documentation>
			<xs:documentation xml:lang="da-DK">Angiver at en lægemiddelordination skal af-seponeres før den bliver opdateret</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="UnwithdrawDrugMedicationOnUpdateType"/>

	<xs:element name="INRStartDate" type="medicinecard20150601:INRStartDateType">
		<xs:annotation>
			<xs:documentation xml:lang="en-GB">The start date of the period wherein the INR-interval is relevant</xs:documentation>
			<xs:documentation xml:lang="da-DK">Startdato for den periode hvor INR-intervallet er relevant</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:simpleType name="INRStartDateType">
		<xs:restriction base="xs:date">
            <xs:minInclusive value="2000-01-01Z"/>
		</xs:restriction>
	</xs:simpleType>

	
	
	
	<xs:element name="DrugStrengthText" type="medicinecard20150601:DrugStrengthTextType">
		<xs:annotation>
			<xs:documentation xml:lang="en-GB">Drug strength, plain text from (LMS01 field 10), not free text</xs:documentation>
			<xs:documentation xml:lang="da-DK">Lægemiddelstyrke, klar tekst fra (LMS01 felt 10), ikke fritekst</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="DrugStrengthTextType">
		<xs:simpleContent>
			<xs:extension base="medicinecard20150601:DrugStrengthTextValueType">
				<xs:attribute name="source" type="medicinecard20150601:DrugStrengthTextSourceType" use="required"/>
				<xs:attribute name="date" type="medicinecard20150601:SourceDateType" use="optional"/>
				<xs:attribute name="yearAndWeek" type="medicinecard20150601:SourceYearAndWeekType" use="optional"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:simpleType name="DrugStrengthTextValueType">
		<xs:restriction base="xs:string">
			<xs:maxLength value="20"/>
